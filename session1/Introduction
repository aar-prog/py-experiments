print('hello world') #to print a string of characters
print(12345) #to print an integer number
print(123.456789) #to print a floating number
fname='Aarush'
lname='Pothula'
name=fname+' '+lname
print(name)
print(type(name)) #combo #string of characters
print(len(name)) #namelength
age=12
print(age)
print(type(age)) #integer
temp=16.5
print(temp)
print(type(temp)) #floating
married=False
print(married)
print(type(married)) #boolian 

print(name, age, temp, married)
print(name)

import math

a=27
b=5
c= math.ceil(a/b)
print(c) #math
print(type(a), type(b), type(c))

name=input("What is your name? \n")
age=input("How old are you? \n")
print("Hello ", name)
print("You are ",age," years old")
c=int(age)+10
print("You will be ",c," years old after 10 years")
